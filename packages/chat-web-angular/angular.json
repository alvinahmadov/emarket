{
	"$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"chat-web": {
			"root": "",
			"sourceRoot": "src",
			"projectType": "application",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"preserveSymlinks": true,
						"outputPath": "www",
						"index": "src/index.html",
						"main": "src/main.ts",
						"tsConfig": "./tsconfig.json",
						"polyfills": "src/polyfills.ts",
						"assets": [
							"src/assets",
							"src/favicon.ico"
						],
						"styles": [
							"src/styles.css",
							"node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css"
						],
						"scripts": [],
						"es5BrowserSupport": true
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								},
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								}
							],
							"optimization": false,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"aot": false,
							"extractLicenses": false,
							"vendorChunk": false,
							"buildOptimizer": false,
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "chat-web-angular:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "chat-web-angular:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "chat-web-angular:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"tsConfig": "src/tsconfig.spec.json",
						"karmaConfig": "src/karma.conf.js",
						"styles": [
							"src/styles.css"
						],
						"scripts": [],
						"assets": [
							"src/favicon.ico",
							"src/assets"
						]
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		},
		"chat-web-e2e": {
			"root": "e2e/",
			"projectType": "application",
			"prefix": "",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "e2e/protractor.conf.js",
						"devServerTarget": "chat-web-angular:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "chat-web-angular:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "e2e/tsconfig.e2e.json",
						"exclude": [
							"**/node_modules/**"
						]
					}
				}
			}
		}
	},
	"defaultProject": "chat-web-angular",
	"cli": {
		"analytics": false
	},
	"schematics": {
		"@schematics/angular:component": {
			"prefix": "ec",
			"style": "scss"
		},
		"@schematics/angular:directive": {
			"prefix": "ec"
		}
	}
}
